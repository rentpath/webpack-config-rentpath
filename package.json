{
  "version": "0.0.7",
  "name": "webpack-config-rentpath",
  "description": "Shared webpack config across our apps",
  "license": "MIT",
  "homepage": "https://github.com/rentpath/webpack-config-rentpath",
  "repository": {
    "type": "git",
    "url": "https://github.com/rentpath/webpack-config-rentpath.git"
  },
  "bugs": {
    "url": "https://github.com/rentpath/webpack-config-rentpath/issues"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf lib",
    "compile": "babel -d lib src",
    "compile:watch": "npm run compile -- --watch",
    "lint": "eslint src test",
    "lint:fix": "npm run lint -- --fix",
    "test": "mocha --compilers js:babel-core/register --timeout 5000 --recursive",
    "test:watch": "npm run test -- --watch",
    "test:cov": "babel-node `npm bin`/istanbul cover `npm bin`/_mocha -- --recursive",
    "preversion": "npm run lint && npm test",
    "version": "npm run compile && git add -A lib",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "assets-webpack-plugin": "^3.1.0",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.9.3",
    "css-loader": "^0.21.0",
    "dotenv": "^2.0.0",
    "exports-loader": "^0.6.2",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "glob": "^6.0.3",
    "handlebars": "^4.0.3",
    "handlebars-loader": "^1.1.4",
    "image-webpack-loader": "^1.6.3",
    "imports-loader": "^0.6.5",
    "node-sass": "^3.3.3",
    "resolve-url-loader": "^1.3.0",
    "sass-loader": "^3.0.0",
    "script-loader": "^0.6.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.2",
    "webpack-asset-manifest": "git+ssh://git@github.com:rentpath/webpack-asset-manifest.git#v0.0.4",
    "webpack-md5-hash": "^0.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.1",
    "eslint": "^2.1.0",
    "eslint-config-rentpath": "^2.0.0",
    "istanbul": "^1.0.0-alpha",
    "mocha": "^2.3.4",
    "jsdom": "8.0.2",
    "jsdom-global": "1.6.1",
    "sinon": "^1.17.3"
  },
  "engines": {
    "node": ">=4.2.1",
    "npm": ">=3"
  }
}
